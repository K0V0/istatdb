
<%= render 'layouts/main_menu' %>

<%= render 'layouts/actions' if @administrative_mode %>

<%= render 'local_taric/tariclist/search' %>

<% if @result.length > 0 %>

	<%= render('layouts/items_table',

		fields: {
			description: {
				is_highlighted: true
			},
			kncode: {
				is_sortlink: true,
				#is_highlighted: true,
				call_function: "num_to_kncode"
			}
		}

	) %>

<% else %>
	
	<%#= render "goods/goodslist/item_no_results", items: @result %>
	<%= render "layouts/items_table_no_results" %>

<% end %>