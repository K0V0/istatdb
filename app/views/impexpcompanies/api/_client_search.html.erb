<% items.each do |item| %>

	<tr <%= return_selected_class_if_in_new_obj_attrs item, :client_filter %>>

		<td> 
			<%= item.company_name %>
		</td>
		
		<td> 
			<%= item.affiliated_office %>
		</td>

	</tr>

<% end %>

<%= render 'layouts/no_results', items: items, err_message: :new_client_search_no_results %>