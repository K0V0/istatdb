$('section.items_table, section.item_details').remove();

<%
	if @result.length > 0
		o = render('layouts/items_table',
			fields: {
				company_name: {
					is_highlighted: true
				}
			}
		) %> 

		$('<%=j o %>').appendTo('main');

	<% else %>

		$('<%=j render("layouts/no_results", items: @result) %>')
		.appendTo('main');

	<% end
%>