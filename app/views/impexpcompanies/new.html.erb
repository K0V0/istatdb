<%= render 'layouts/main_menu' %>

<section class="form">

	<%  url = update_impexpcompany_path if action_name == "edit"
		url = create_impexpcompany_path if action_name == "new" %>

	<%= form_for @impexpcompany, url: url do |t| %>

		<article class="decorated_wrap shrinked">

			<%= render('layouts/new_select_section', 
				obj: t,
				input_fields: { 
					company_name: {
						required: true,
						autocomplete: false,
						reset_button: true,
						autofocus: true
					}
				}
			) %>

		</article>

		<article class="decorated_wrap shrinked">

			<div>
				<%= t.submit %>
			</div>

		</article>

	<% end %>

</section>