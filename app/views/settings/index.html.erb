<%= render 'settings/shared/settings_submenu' %>

<%= render 'layouts/base/main_menu' %>

<article class="settings">

	<%= form_tag settings_path, remote: true do %>

		<section class="decorated_wrap shrinked">

			<table class="for_nontabular">

				<caption> 
					<%= t('settings.general.gui_section') %>
				</caption>

				<tbody>
					<tr> <%= render 'settings/misc/per_page' %> </tr>
					<tr> <%= render 'settings/misc/show_footer' %> </tr>
				</tbody>

			</table>

		</section>

	<% end %>

</article>




