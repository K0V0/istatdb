
<% items.each do |item| %>

	<tr>

		<td> 
			<%= link_to item, remote: true do %>
				<%= highlight_search item, :ident %>
			<% end %>
		</td>
		
		<td> <%= item.description %> </td>
		<td> <%#= item.local_taric.kncode %> </td>
		<td> <%#= item.local_taric.description %> </td>

	</tr>

<% end %>

<%= render 'layouts/no_results', items: items %>