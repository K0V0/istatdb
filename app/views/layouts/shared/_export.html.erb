<% 
obj = nil if !defined?(obj)
path_back = defined?(backlink) ? backlink : { action: :index, controller: controller_name }
%>

<%= form_tag(public_send("do_export_#{controller_name}_path"), target: '_blank', method: :post) do |f| %>

    <%= hidden_field_tag 'head', head.to_json %>

    <% if defined?(@export_options) %>

        <% @export_options.each do |k, o| %>

            <%= hidden_field_tag(k, o) %>

        <% end %>

    <% end %>

    <div class="decorated_wrap shrinked">

        <table class="with_controls export">

            <caption>Export</caption>

            <thead></thead>

            <tbody>

                <%= render(
                    'layouts/shared/export/export_properties_line',
                    field: fields_to_select,
                    obj: obj
                ) %>

            </tbody>

            <tfoot>
                <tr>
                    <td colspan="10">
                        <input type="submit" class="button" value="Excel tabuÄ¾ka" name="submit_excel">
                        <input type="submit" class="button" value="HTML" name="submit_html">
                        <%= link_to "Storno", path_back, class: 'button' %>
                    </td>
                </tr>
            </tfoot>

        </table>

    </div>
<% end %>
