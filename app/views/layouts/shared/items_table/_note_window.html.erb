<%
	html_class = ""
	if defined?(not_resolved)
		html_class = not_resolved ? "not_resolved" : ""
	end
%>

<section class="decorated_wrap shrinked items_table <%= html_class %>">

	<table class="rows_colored no_line_height">

		<caption>
			<%= caption %>
		</caption>

		<tbody>
			<tr>
				<td>
					<%= content %>
				</td>
			</tr>
		</tbody>

		<tfoot>
			<tr>
				<th colspan="99">
					<div id="items_table_total_count"> 
						<%= foot_content %>
					</div>
				</th>
			</tr>
		</tfoot>

	</table>

</section>