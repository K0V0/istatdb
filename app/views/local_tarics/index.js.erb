
$('section.items_table, section.item_details').remove();


<%
	if @result.length > 0

		o = render('layouts/items_table',
			fields: {
				description: {
					is_highlighted: true
				},
				kncode: {
					is_sortlink: true,
					is_highlighted: true,
					call_function: "num_to_kncode",
					clipboard_button: true
				}
			}
		) %> 

		$('<%=j o %>').appendTo('main');

	<% else %>

		$('<%=j render("layouts/items_table_no_results") %>')
		.appendTo('main');

	<% end
%>